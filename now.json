{
  "version": 2,
  "name": "lorem-ipsum",
  "builds": [
    { "src": "www/next.config.js", "use": "@now/next" },
    { "src": "api/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/www/$1" }
  ],
  "env": {
    "AUTH0_CLIENT_ID": "@auth0-client-id",
    "AUTH0_CLIENT_DOMAIN": "@auth0-client-domain"
  }
}
